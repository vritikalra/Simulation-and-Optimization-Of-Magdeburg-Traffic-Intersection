			<Functions>
				<Function AccessType="default" StaticFunction="false">
					<ReturnModificator>VOID</ReturnModificator>
					<ReturnType><![CDATA[double]]></ReturnType>
					<Id>1421751912508</Id>
					<Name><![CDATA[move]]></Name>
					<X>330</X><Y>140</Y>
					<Label><X>10</X><Y>0</Y></Label>
					<PublicFlag>false</PublicFlag>
					<PresentationFlag>true</PresentationFlag>
					<ShowLabel>true</ShowLabel>
					<Body><![CDATA[//determine next position
x += getSpeed() * cos(heading);
y += getSpeed() * sin(heading); 
// the 2D space is made a torus
if (x > main.spaceWidth())
  x -= main.spaceWidth();
else if (x < 0)
  x += main.spaceWidth();
if (y > main.spaceHeight())
  y -= main.spaceHeight();
else if (y < 0)
  y += main.spaceHeight();
//modify bird agent
setXYZ(x, y, 0);
setRotation(heading);]]></Body>
				</Function>
				<Function AccessType="default" StaticFunction="false">
					<ReturnModificator>VOID</ReturnModificator>
					<ReturnType><![CDATA[double]]></ReturnType>
					<Id>1421751912520</Id>
					<Name><![CDATA[adjust_heading]]></Name>
					<X>200</X><Y>130</Y>
					<Label><X>10</X><Y>0</Y></Label>
					<PublicFlag>false</PublicFlag>
					<PresentationFlag>true</PresentationFlag>
					<ShowLabel>true</ShowLabel>
					<Body><![CDATA[if(getConnectionsNumber() == 0) return;

//find closest neighbor
double closest_head = heading;
double dist = 100, new_dist;
for(Agent a : getConnections()){
	new_dist = distanceTo(a);
	if(new_dist < dist){ 
		dist = new_dist;
		closest_head = ((Bird) a).heading;
	}
}

if(dist < crowded){ //short range repulsion
	//bird.setFillColor(red);
	separate(closest_head);
}else{ //long range attraction
	align();
	cohere();
}]]></Body>
				</Function>
				<Function AccessType="default" StaticFunction="false">
					<ReturnModificator>VOID</ReturnModificator>
					<ReturnType><![CDATA[double]]></ReturnType>
					<Id>1421751912525</Id>
					<Name><![CDATA[turn_towards]]></Name>
					<X>200</X><Y>190</Y>
					<Label><X>10</X><Y>0</Y></Label>
					<PublicFlag>false</PublicFlag>
					<PresentationFlag>true</PresentationFlag>
					<ShowLabel>true</ShowLabel>
					<Parameter>
						<Name><![CDATA[others_heading]]></Name>
						<Type><![CDATA[double]]></Type>
					</Parameter>
					<Body><![CDATA[double diff = subtract_headings(heading, others_heading);
if(diff == 0) return;
//determine new heading
if(diff > 0) heading += min(diff,max_turn);
else heading += max(diff,-max_turn);
//normalize
if(heading < 0) heading += 2*PI;
if(heading >= 2*PI) heading -= 2*PI;]]></Body>
				</Function>
				<Function AccessType="default" StaticFunction="false">
					<ReturnModificator>RETURNS_VALUE</ReturnModificator>
					<ReturnType><![CDATA[double]]></ReturnType>
					<Id>1421751912529</Id>
					<Name><![CDATA[subtract_headings]]></Name>
					<X>320</X><Y>180</Y>
					<Label><X>10</X><Y>0</Y></Label>
					<PublicFlag>false</PublicFlag>
					<PresentationFlag>true</PresentationFlag>
					<ShowLabel>true</ShowLabel>
					<Parameter>
						<Name><![CDATA[heading1]]></Name>
						<Type><![CDATA[double]]></Type>
					</Parameter>
					<Parameter>
						<Name><![CDATA[heading2]]></Name>
						<Type><![CDATA[double]]></Type>
					</Parameter>
					<Body><![CDATA[//determine and normalize heading
double diff = heading2-heading1;
if(diff <= -PI) diff += 2*PI;
if(diff > PI) diff -= 2*PI;
return diff;]]></Body>
				</Function>
				<Function AccessType="default" StaticFunction="false">
					<ReturnModificator>VOID</ReturnModificator>
					<ReturnType><![CDATA[double]]></ReturnType>
					<Id>1421751912548</Id>
					<Name><![CDATA[separate]]></Name>
					<X>200</X><Y>160</Y>
					<Label><X>10</X><Y>0</Y></Label>
					<PublicFlag>false</PublicFlag>
					<PresentationFlag>true</PresentationFlag>
					<ShowLabel>true</ShowLabel>
					<Parameter>
						<Name><![CDATA[others_heading]]></Name>
						<Type><![CDATA[double]]></Type>
					</Parameter>
					<Body><![CDATA[double diff = subtract_headings(heading, others_heading);
if(diff == 0) return;
//determine new heading
if(diff > 0) heading -= min(diff,max_turn_away);
else heading -= max(diff,-max_turn_away);
//normalize
if(heading < 0) heading += 2*PI;
if(heading > 2*PI) heading -= 2*PI;]]></Body>
				</Function>
				<Function AccessType="default" StaticFunction="false">
					<ReturnModificator>VOID</ReturnModificator>
					<ReturnType><![CDATA[double]]></ReturnType>
					<Id>1421751912555</Id>
					<Name><![CDATA[align]]></Name>
					<X>200</X><Y>210</Y>
					<Label><X>10</X><Y>0</Y></Label>
					<PublicFlag>false</PublicFlag>
					<PresentationFlag>true</PresentationFlag>
					<ShowLabel>true</ShowLabel>
					<Body><![CDATA[double avg_head = 0, avg_x = cos(heading), avg_y = sin(heading);
for(Agent a : getConnections()){
	avg_x += cos(((Bird)a).heading);
	avg_y += sin(((Bird)a).heading);
}
avg_x /= getConnectionsNumber()+1;
avg_y /= getConnectionsNumber()+1;
avg_head = atan2(avg_y, avg_x);
turn_towards(avg_head);]]></Body>
				</Function>
				<Function AccessType="default" StaticFunction="false">
					<ReturnModificator>VOID</ReturnModificator>
					<ReturnType><![CDATA[double]]></ReturnType>
					<Id>1421751912563</Id>
					<Name><![CDATA[cohere]]></Name>
					<X>200</X><Y>230</Y>
					<Label><X>10</X><Y>0</Y></Label>
					<PublicFlag>false</PublicFlag>
					<PresentationFlag>true</PresentationFlag>
					<ShowLabel>true</ShowLabel>
					<Body><![CDATA[// align flight pattern
double avg_pos_x = x, avg_pos_y = y;
